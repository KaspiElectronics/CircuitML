{% if left[i] != right[i] %}
    if (x[{{ features[i] }}] <= {{ thresholds[i] }}) {
        {% with i = left[i] %}
            {% include 'decisiontree/tree_regressor.jinja' %}
        {% endwith %}
    }
    else {
        {% with i = right[i] %}
            {% include 'decisiontree/tree_regressor.jinja' %}
        {% endwith %}
    }
{% else %}
    return {{ values[i].mean() }}f;
{% endif %}